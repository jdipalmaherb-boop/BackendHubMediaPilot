{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "prisma generate --schema=prisma/schema.prisma && tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "postinstall": "prisma migrate deploy && prisma generate"
  }
}

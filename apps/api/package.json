{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "prisma generate --schema=prisma/schema.prisma && tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "postinstall": "pnpm exec prisma migrate deploy && pnpm exec prisma generate"
  },
  "dependencies": {
    "prisma": "^7.2.0"
  }
}

    "@types/node": "20.11.30",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "eslint": "9.5.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "4.11.0",
    "typescript": "5.4.5"
  }
